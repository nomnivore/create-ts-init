{
  "name": "create-ts-init",
  "version": "0.2.0",
  "description": "A simple boilerplate generator for TypeScript projects that helps you save time on configuration.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/nomnivore/create-ts-init",
  "main": "./dist/index.js",
  "bin": {
    "create-ts-init": "./dist/index.js"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "prepack": "npm run build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fmt": "npm run format && npm run lint"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/node": "^18.15.5",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "2.8.4"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "execa": "^7.1.1",
    "fs-extra": "^11.1.1",
    "inquirer": "^9.1.5",
    "ora": "^6.2.0"
  },
  "keywords": [
    "typescript",
    "starter",
    "boilerplate",
    "template",
    "cli",
    "scaffold",
    "init",
    "initialization"
